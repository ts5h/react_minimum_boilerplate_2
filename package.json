{
  "name": "react_minimum_boilerplate_2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "biome format --write ./src && prettier --write \"./src/**/*.scss\"",
    "lint": "biome lint ./src",
    "lint:fix": "biome check --write ./src",
    "preview": "vite preview",
    "start": "pnpm run dev"
  },
  "dependencies": {
    "jotai": "^2.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "prettier": "3.3.3",
    "sass": "^1.80.4",
    "typescript": "~5.6.2",
    "vite": "^5.4.9"
  }
}
